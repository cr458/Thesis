@Article{Ramvall2000,
  Title                    = {{Optical properties of GaN quantum dots}},
  Author                   = {Ramvall, P and Riblet, P and Nomura, S and Aoyagi, Y and Tanaka, S},
  Journal                  = {Journal of Applied Physics},
  Year                     = {2000},
  Number                   = {2000},
  Pages                    = {3883--3890},
  Volume                   = {87},

  Abstract                 = {We report on an investigation of the optical properties of GaN quantum dots (QDs) grown by means of metalorganic vapor phase epitaxy. The growth regime for GaN on AlxGa1-xN was observed to change from two- to three-dimensional, forming GaN QDs, when Si was deposited on the AlxGa1-xN surface prior to the GaN growth. These QDs showed a redshift of the photo luminescence (PL) energy from the increased Coulomb energy induced by a compression of the exciton Bohr radius. Furthermore, a diminishing temperature-dependent shift of the PL energy with decreasing QD size caused by a reduction of the longitudinal-optical phonon coupling was found. We also show that the size of the QDs is a critical parameter for the optical nonlinearities. For large dots, the dominant nonlinearity in the PL is the bandgap renormalization but when the size of the dots was reduced below the critical size of 10 nm thick and 30 nm diameter, the state-filling effect became dominant. (C) 2000 American Institute of Physics. [S0021-8979(00)04708-3].},
  Doi                      = {10.1063/1.372429},
  File                     = {:C$\backslash$:/Users/cr458/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ramvall et al. - 2000 - Optical properties of GaN quantum dots.pdf:pdf},
  ISBN                     = {0021-8979},
  ISSN                     = {00218979},
  Keywords                 = {Conduction Band,Dielectric Function,Emission,Exciton Binding Energy,Laser,Low Threshold,Molecular Beam Epitaxy,Resonance Raman,Semiconductor Microcrystallites,Temperature Dependence},
  Mendeley-groups          = {JAP special topic/Response,thesis}
}

