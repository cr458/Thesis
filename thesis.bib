@Article{Cho1998,
  Title                    = {{"S-shaped" temperature-dependent emission shift and carrier dynamics in InGaN/GaN multiple quantum wells}},
  Author                   = {Cho, Yong Hoon and Gainer, G. H. and Fischer, A. J. and Song, J. J. and Keller, S. and Mishra, U. K. and DenBaars, S. P.},
  Journal                  = {Applied Physics Letters},
  Year                     = {1998},
  Number                   = {10},
  Pages                    = {1370--1372},
  Volume                   = {73},

  Abstract                 = {We report temperature-dependent time-integrated and time-resolved photoluminescence (PL) studies of InGaN/GaN multiple quantum wells (MQWs) grown by metalorganic chemical vapor deposition. We observed anomalous emission behavior, specifically an /b S-shaped/ (decrease-increase-decrease) temperature dependence of the peak energy (E/sub p/) for InGaN-related PL with increasing temperature: E/sub p/ redshifts in the temperature range of 10-70 K, blueshifts for 70-150 K, and redshifts again for 150-300 K with increasing temperature. In addition, when E/sub p/ redshifts, the spectral width is observed to narrow, while when E/sub p/ blueshifts, it broadens. From a study of the integrated PL intensity as a function of temperature, it is found that thermionic emission of photocarriers out of local potential minima into higher energy states within the wells is the dominant mechanism leading to the thermal quenching of the InGaN-related PL. We demonstrate that the temperature-induced S-shaped PL shift is caused by a change in the carrier dynamics with increasing temperature due to inhomogeneity and carrier localization in the InGaN/GaN MQWs},
  Doi                      = {10.1063/1.122164},
  File                     = {:C$\backslash$:/Users/cr458/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Cho et al. - 1998 - S-shaped temperature-dependent emission shift and carrier dynamics in InGaNGaN multiple quantum wells.pdf:pdf},
