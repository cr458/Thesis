  Keywords                 = {AlN,GaN,InN,LED,Laser,Nanowire,Optoelectronics,Photodetector,Photosynthesis,Si photonics,Solar cell,Solar fuel,Solar hydrogen,Water splitting},
  Mendeley-groups          = {thesis},
  Publisher                = {Elsevier},
  Url                      = {http://dx.doi.org/10.1016/j.pquantelec.2015.11.001}
}

