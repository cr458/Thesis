}

@Article{Saba2001,
  Title                    = {{High-temperature ultrafast polariton parametric amplification in semiconductor microcavities.}},
  Author                   = {Saba, M and Ciuti, C and Bloch, J and Thierry-Mieg, V and Andr{\'{e}}, R and Dang, Le S and Kundermann, S and Mura, A and Bongiovanni, G and Staehli, J L and Deveaud, B},
  Journal                  = {Nature},
  Year                     = {2001},
  Number                   = {6865},
  Pages                    = {731--735},
  Volume                   = {414},

  Abstract                 = {Cavity polaritons, the elementary optical excitations of semiconductor microcavities, may be understood as a superposition of excitons and cavity photons. Owing to their composite nature, these bosonic particles have a distinct optical response, at the same time very fast and highly nonlinear. Very efficient light amplification due to polariton-polariton parametric scattering has recently been reported in semiconductor microcavities at liquid-helium temperatures. Here we demonstrate polariton parametric amplification up to 120 K in GaAlAs-based microcavities and up to 220 K in CdTe-based microcavities. We show that the cut-off temperature for the amplification is ultimately determined by the binding energy of the exciton. A 5-micrometer-thick planar microcavity can amplify a weak light pulse more than 5,000 times. The effective gain coefficient of an equivalent homogeneous medium would be 107 cm-1. The subpicosecond duration and high efficiency of the amplification could be exploited for high-repetition all-optical microscopic switches and amplifiers. 105 polaritons occupy the same quantum state during the amplification, realizing a dynamical condensate of strongly interacting bosons which can be studied at high temperature.},
  Doi                      = {10.1038/414731a},
  File                     = {:C$\backslash$:/Users/cr458/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Saba et al. - 2001 - High-temperature ultrafast polariton parametric amplification in semiconductor microcavities.pdf:pdf},
  ISBN                     = {0028-0836 (Print)$\backslash$r0028-0836 (Linking)},
  ISSN                     = {00280836},
  Mendeley-groups          = {JAP special topic/Cavities,thesis},
  Pmid                     = {11742394}
}

