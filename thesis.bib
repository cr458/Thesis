@Article{Holzer2004,
  Title                    = {{Three-dimensional analysis of porous $BaTiO_{3}$ ceramics using FIB nanotomography}},
  Author                   = {Holzer, Lorenz and Indutnyi, F. and Gasser, Ph and M{\"{u}}nch, B. and Wegmann, M.},
